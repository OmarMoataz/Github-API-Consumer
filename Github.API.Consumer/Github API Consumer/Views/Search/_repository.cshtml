<table class="table table-dark col-10">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Description</th>
            <th scope="col">Language</th>
            <th scope="col"> Url </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {

            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Description</td>
                <td>@item.Language</td>
                <td><a href="@item.HtmlUrl"> Click here </a> </td>
            </tr>
        }
    </tbody>
</table>